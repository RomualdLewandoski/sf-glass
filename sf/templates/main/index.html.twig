{% extends 'base.html.twig' %}

{% block title %}Hello MainController!{% endblock %}
{% block stylesheets %} {{ parent() }} {% endblock %}
{% block body %}



    {% embed "frontEmbed/header.twig" %}
        {% block headerTitle %}
            {{ mainConfig.headerTitle }}
        {% endblock %}
        {% block headerSub %}
            {{ mainConfig.headerSub }}
        {% endblock %}

        {% block imgUrl %}
            {{ vich_uploader_asset(mainConfig.headerBg, 'imageFile') }}
        {% endblock %}
    {% endembed %}

    {% embed "frontEmbed/about.twig" %}
    {% endembed %}

    <section class="features1 cid-s0bsg6BUXn" id="features01-2">

        <div class="container-fluid mbr-white">
            <div class="mbr-row">
                <div class="mbr-col-lg-6 mbr-col-md-6  mbr-m-auto mbr-col-sm-12" style="position: relative;
                        background-image: url('{{ vich_uploader_asset(mainConfig.gender1Bg, "imageFile") }}');
                        background-size: cover;
                        background-position: center;
                        ">
                    <div class="mbr-overlay"></div>
                    <div class="content">


                        <h1 class="mbr-section-title mbr-fonts-style align-left mbr-pb-2 display-2">
                            <strong>{{ mainConfig.gender1Title }}</strong></h1>

                        <div class="mbr-section-btn mbr-pt-5 align-left"><a
                                    class="btn btn-md btn-white-outline display-4"
                                    href="{{ path('view_gender', {"slug":mainConfig.gender1slug}) }}"><span
                                        class="mobi-mbri mobi-mbri-right mbr-iconfont mbr-iconfont-btn"><svg
                                            xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="currentColor"
                                            width="19" height="19"><path
                                                d="M24 11l-6.7 6.7-1.4-1.4 4.3-4.3H0v-2h20.2l-4.3-4.3 1.4-1.4L24 11z"></path></svg></span>Shop
                                Now</a>
                        </div>
                    </div>
                </div>
                <div class="mbr-col-lg-6 mbr-col-md-6 mbr-m-auto mbr-col-sm-12" style="position: relative;
                        background-image: url('{{ vich_uploader_asset(mainConfig.gender2Bg, "imageFile") }}');
                        background-size: cover;
                        background-position: center;
                        ">
                    <div class="mbr-overlay"></div>
                    <div class="content">


                        <h1 class="mbr-section-title mbr-fonts-style align-left mbr-pb-2 display-2">
                            <strong>{{ mainConfig.gender2Title }}</strong></h1>

                        <div class="mbr-section-btn mbr-pt-5 align-left"><a
                                    class="btn btn-md btn-white-outline display-4"
                                    href="{{ path('view_gender', {"slug":mainConfig.gender2slug}) }}"><span
                                        class="mobi-mbri mobi-mbri-right mbr-iconfont mbr-iconfont-btn"><svg
                                            xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="currentColor"
                                            width="19" height="19"><path
                                                d="M24 11l-6.7 6.7-1.4-1.4 4.3-4.3H0v-2h20.2l-4.3-4.3 1.4-1.4L24 11z"></path></svg></span>Shop
                                Now</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>


    {% embed 'frontEmbed/care.twig' %}
    {% endembed %}


    <section class="info1 cid-s0bsn70ibR" id="info01-5">


        <div class="container">
            <div class="mbr-row">
                <div class="mbr-col-lg-12 mbr-col-md-12 mbr-m-auto mbr-col-sm-12 md-pb">
                    <div class="content">
                        <h4 class="mbr-section-subtitle mbr-fonts-style align-center display-4">GIFT GUIDE</h4>
                        <h3 class="mbr-section-title mbr-fonts-style align-center mbr-pb-4 display-2">Shop All
                            Gifts</h3>

                        <div class="mbr-section-btn align-center"><a class="btn btn-lg btn-black display-4" href="#">Shop</a>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </section>

    {% embed "frontEmbed/promoProduct.twig" %}
    {% endembed %}

    <section class="features2 cid-s0bsJ2zwJy" id="features02-b" style="
            background-image: url('{{ vich_uploader_asset(siteConfig.howWorksBg, "imageFile") }}');
            ">

        <div class="mbr-overlay"></div>
        <div class="container mbr-white">
            <div class="mbr-row">
                <div class="mbr-col-lg-6 mbr-col-md-12 mbr-col-sm-12">
                    <h3 class="mbr-section-title mbr-fonts-style align-left mbr-pb-4 display-5">How it Works</h3>
                    <div class="mbr-text counter-container mbr-fonts-style mbr-pb-1 display-7">
                        {{ siteConfig.howWorks|raw }}
                    </div>
                </div>
            </div>
        </div>
    </section>

    <section class="features3 mbr-section cid-s0bt3IU31C" id="features03-d">


        <div class="container">
            <div class="mbr-row mbr-jc-c">
                <div class="mbr-col-lg-6 mbr-pb-5 mbr-col-md-10 mbr-col-sm-12">

                    <h2 class="main-title mbr-fonts-style mbr-pb-2 align-center display-2">Our Features</h2>
                </div>
            </div>
            <div class="mbr-row mbr-jc-c">
                {% for feature in features %}
                    <div class="card md-pb mbr-col-sm-12 mbr-col-md-6 mbr-col-lg-4">
                        <div class="card-wrapper">
                            <div class="card-img">
                                <amp-img src="{{ vich_uploader_asset(feature.img, 'imageFile') }}" layout="responsive" width="718.0851063829788"
                                         height="600" alt="" class="mobirise-loader">
                                    <div placeholder="" class="placeholder">
                                        <div class="mobirise-spinner">
                                            <em></em>
                                            <em></em>
                                            <em></em>
                                        </div>
                                    </div>

                                </amp-img>
                            </div>
                            <div class="card-box">
                                <h4 class="card-title mbr-fonts-style mbr-pb-1 display-7">{{ feature.titre }}</h4>
                                <p class="mbr-text mbr-fonts-style mbr-semibold display-4">
                                    <a href=""> Read more</a></p>
                            </div>

                        </div>
                    </div>
                {% endfor %}


            </div>
        </div>
    </section>

    <section class="content3 cid-s0bsiqoLOf" id="content03-3">


        <div class="container">
            <div class="mbr-row">

                <div class="mbr-col-lg-7 mbr-col-md-12  mbr-m-auto mbr-col-sm-12 md-pb">
                    <div class="img-wrapper">
                        <amp-img src="assets/images/02.jpg" alt="" width="766" height="766" layout="responsive"
                                 class="mobirise-loader mbr-m-auto align-center">
                            <div placeholder="" class="placeholder">
                                <div class="mobirise-spinner">
                                    <em></em>
                                    <em></em>
                                    <em></em>
                                </div>
                            </div>

                        </amp-img>
                    </div>
                </div>


                <div class="mbr-col-lg-5 mbr-col-md-12 mbr-m-auto mbr-col-sm-12">
                    <div class="content">

                        <h1 class="mbr-section-title mbr-fonts-style align-left mbr-pb-2 display-2">Your Everyday
                            Sunglasses</h1>
                        <p class="mbr-fonts-style mbr-text align-left mbr-pb-5  display-7">Lorem ipsum dolor sit amet,
                            consectetur adipiscing elit. Nunc commodo ligula eget ex accumsan, in ultricies tellus
                            vehicula
                        </p>
                        <div class="mbr-section-btn align-left"><a class="btn btn-md btn-warning-outline display-4"
                                                                   href="#"><span
                                        class="mobi-mbri mobi-mbri-right mbr-iconfont mbr-iconfont-btn"><svg
                                            xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="currentColor"
                                            width="19" height="19"><path
                                                d="M24 11l-6.7 6.7-1.4-1.4 4.3-4.3H0v-2h20.2l-4.3-4.3 1.4-1.4L24 11z"></path></svg></span>Shop
                                Now</a>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </section>

    {% embed "frontEmbed/talkingUs.twig" %}
    {% endembed %}

    {% embed "frontEmbed/contactForm.twig" %}
    {% endembed %}

    {% embed "frontEmbed/map.twig" %}
    {% endembed %}

{% endblock %}
{% block javascripts %} {{ parent() }} {% endblock %}

